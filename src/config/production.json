    "enablePerformanceMonitoring": true,
    "enableCrashlytics": true,
    "enableOfflineSupport": true,
    "enablePushNotifications": true,
    "enableAdvancedSearch": true,
    "enableBulkOperations": true,
    "enableReports": true,
