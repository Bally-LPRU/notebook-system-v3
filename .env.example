# Environment Configuration
REACT_APP_ENVIRONMENT=development

# Development Firebase Configuration
REACT_APP_FIREBASE_API_KEY_DEV=your_dev_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN_DEV=your_dev_project_id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID_DEV=your_dev_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET_DEV=your_dev_project_id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID_DEV=your_dev_sender_id
REACT_APP_FIREBASE_APP_ID_DEV=your_dev_app_id
REACT_APP_FIREBASE_MEASUREMENT_ID_DEV=your_dev_measurement_id

# Production Firebase Configuration
REACT_APP_FIREBASE_API_KEY_PROD=your_prod_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN_PROD=your_prod_project_id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID_PROD=your_prod_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET_PROD=your_prod_project_id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID_PROD=your_prod_sender_id
REACT_APP_FIREBASE_APP_ID_PROD=your_prod_app_id
REACT_APP_FIREBASE_MEASUREMENT_ID_PROD=your_prod_measurement_id

# Fallback Firebase Configuration (for backward compatibility)
REACT_APP_FIREBASE_API_KEY=your_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id
REACT_APP_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Development Settings
REACT_APP_USE_EMULATOR=true

# Production Settings (set these in production environment)
# REACT_APP_USE_EMULATOR=false
# REACT_APP_ENVIRONMENT=production